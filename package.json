      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "@next/font": "13.2.4",
    "eslint": "^8.36.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4"
  }
}
